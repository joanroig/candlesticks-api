{
  "name": "candlesticks-api",
  "version": "0.1.0",
  "main": "index.ts",
  "repository": "https://github.com/joanroig/candlesticks-api.git",
  "author": "Joan Roig <joanroigsegura@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./build && tsc -p .",
    "start": "node build/index.js",
    "start:dev": "nodemon",
    "start:partner-service": "java -jar partner-service-1.0.1-all.jar --port=8032",
    "lint": "eslint . --ext .ts",
    "format": "eslint . --ext .ts --fix",
    "format:staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "config": "^3.3.7",
    "express": "^4.18.1",
    "moment": "^2.29.3",
    "nodemon": "^2.0.16",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "winston": "^3.7.2",
    "ws": "^8.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@types/config": "0.0.41",
    "@types/express": "4.17.13",
    "@types/node": "17.0.31",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "eslint": "8.15.0",
    "husky": "7.0.4",
    "lint-staged": "12.4.1",
    "rimraf": "3.0.2"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": "^1.22.17"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": "eslint --cache --fix"
  }
