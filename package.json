{
  "name": "candlesticks-api",
  "version": "1.0.0",
  "main": "index.ts",
  "repository": "https://github.com/joanroig/candlesticks-api.git",
  "author": "Joan Roig <joanroigsegura@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./build && tsc -p tsconfig.build.json",
    "start": "node build/index.js",
    "start:dev": "nodemon",
    "start:partner-service": "java -jar partner-service-1.0.1-all.jar --port=8032",
    "docker:build": "docker-compose build --no-cache",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "test": "jest",
    "test:coverage": "jest --detectOpenHandles --watchAll --collectCoverage",
    "test:ci": "yarn run lint && jest --ci --collectCoverage",
    "lint": "eslint . --ext .ts",
    "format": "eslint . --ext .ts --fix",
    "format:staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.13.2",
    "class-validator-jsonschema": "^3.1.0",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "jest": "^28.1.0",
    "moment": "^2.29.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.16",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.1.0",
    "swagger-ui-express": "^4.3.0",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typedi": "^0.10.0",
    "typescript": "^4.6.4",
    "winston": "^3.7.2",
    "ws": "^8.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@types/config": "0.0.41",
    "@types/express": "4.17.13",
    "@types/jest": "27.5.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "17.0.31",
    "@types/supertest": "2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "13.7.2",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "eslint": "8.15.0",
    "husky": "8.0.1",
    "lint-staged": "12.4.1",
    "rimraf": "3.0.2",
    "supertest": "6.2.3",
    "typed-emitter": "^2.1.0"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": "^1.22.17"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": "eslint --cache --fix"
  }
}
